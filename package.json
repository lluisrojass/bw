{
  "name": "bw",
  "version": "1.0.0",
  "description": "An black and white image blog",
  "main": "index.js",
  "scripts": {
    "import-image": "node ./scripts/import-image.js",
    "create-build-dir": "mkdir -p ./build",
    "create-meta-dir": "mkdir -p ./src/static/images/meta",
    "create-image-variants": "node ./scripts/create-image-variants",
    "create-image-meta": "npm run create-meta-dir && node ./scripts/create-image-meta",
    "nuxt-dev": "nuxt",
    "nuxt-generate": "nuxt generate",
    "dev": "npm run create-build-dir && npm run nuxt-dev",
    "generate": "npm run create-build-dir && npm run generate",
    "paginate-stream": "cross-env PAGE_SIZE=10 node ./scripts/create-pagination.js",
    "clear-pages": "rimraf ./src/static/directories && rimraf ./src/static/pages",
    "setup-pages": "mkdir -p ./src/static/directories && mkdir -p ./src/static/pages",
    "paginate": "npm run clear-pages && npm run setup-pages && npm run paginate-stream",
    "prepare-build": "npm run generate-image-variants && npm run paginate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lluisrojass/bw.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lluisrojass/bw/issues"
  },
  "homepage": "https://github.com/lluisrojass/bw#readme",
  "dependencies": {
    "axios": "^0.21.4",
    "nuxt": "^2.15.8",
    "typescript": "^4.3.5",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@nuxtjs/svg": "^0.2.0",
    "cross-env": "^7.0.3",
    "git-repo-info": "^2.1.1",
    "jimp": "^0.16.1",
    "md5": "^2.3.0",
    "rimraf": "^3.0.2",
    "sass": "^1.39.0",
    "sass-loader": "^10.1.1",
    "yaml": "^1.10.2"
  }
}
